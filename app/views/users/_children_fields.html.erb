<div class="nested-fields">
 <div class="row field ">
 	<div class = 'col-xs-4 col-md-4'>
	   <%= f.label :children_name %><br>
	   <%= f.label :children_aadhar_number %><br>
	   <%= f.label :children_bdate %>
	   <%= f.fields_for :documents, Document.new do |document| %>
			<%= document.file_field :attachment %>
		<% end %>
	</div>
	<div class = 'col-xs-4 col-md-4'>
		<%= f.text_field :children_name %>
		<%= f.text_field :children_aadhar_number %>
		<%= f.text_field :children_birthdate, class: "datepicker"%>
	</div>
 </div>
 <%= link_to_remove_association "Remove Child", f%>
</div>


